@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Add any required dependencies here
# find_dependency(SomePackage)

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/ptensorTargets.cmake")

# Create aliases if not already defined
if(NOT TARGET p10::core)
    add_library(p10::core ALIAS ptensor)
endif()

if(TARGET ptensor_io AND NOT TARGET p10::io)
    add_library(p10::io ALIAS ptensor_io)
endif()

if(TARGET ptensor_testing AND NOT TARGET p10::testing)
    add_library(p10::testing ALIAS ptensor_testing)
endif()

if(TARGET ptensor_op AND NOT TARGET p10::op)
    add_library(p10::op ALIAS ptensor_op)
endif()

check_required_components(ptensor)